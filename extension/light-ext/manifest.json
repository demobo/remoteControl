{
   "name": "Berry",
   "description": "Berry video call",
   "browser_action":
   {
	  "default_icon": "images/icon_bw_19.png",
      "default_title": "berry",
	  "default_popup": "popup.html"
   },
   "background" : {
    "scripts": ["js/background.js"]
   },
   "content_scripts": [
      {
         "all_frames": false,
         "js": ["js/main.js"],
		 "css": ["css/main.css"],
		 "matches": [ "http://localhost:9090/*" , "http://www.baidu.com/", "http://grooveshark.com/", "http://rc1.demobo.com/"],
         "run_at": "document_end"
      }
   ],
   "icons":
   {
   	 "128": "images/icon_128.png",
      "48": "images/icon_48.png",
	  "19": "images/icon_bw_19.png",
      "16": "images/icon_bw_16.png"
   },
   "homepage_url": "http://www.demobo.com",
   "permissions": [
     "http://localhost:9090/*", "http://www.baidu.com/", "http://grooveshark.com/"
   ],
   "version": "3.2.1",
   "manifest_version": 2
}
